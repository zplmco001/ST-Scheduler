<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/pop_bg"
    tools:context="com.trial.kk.trial.PopResult"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp"
    android:focusable="true"
    android:focusableInTouchMode="true">

    <Button
        android:id="@+id/button3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        android:layout_marginEnd="12dp"
        android:layout_marginStart="11dp"
        android:text="KAYDET"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/scrollView2"
        app:layout_constraintRight_toLeftOf="@+id/button4"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <Button
        android:id="@+id/button4"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        android:layout_marginEnd="16dp"
        android:text="GERİ DÖN"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintRight_creator="1" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="5dp"
        android:layout_marginEnd="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="4dp"
        app:layout_constraintBottom_toTopOf="@+id/button3"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1">

        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <EditText
                android:id="@+id/tythampuan"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_column="0"
                android:layout_row="0"
                android:background="@android:color/transparent"
                android:ems="10"
                android:enabled="false"
                android:inputType=""
                android:text="SELAM TEST"
                android:textColor="#000"
                android:textSize="24sp" />

            <EditText

                android:id="@+id/tytyerpuan"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_column="0"
                android:layout_row="1"
                android:background="@android:color/transparent"
                android:ems="10"
                android:enabled="false"
                android:inputType=""
                android:textColor="#000"
                android:textSize="24sp" />

            <EditText

                android:id="@+id/mfhampuan"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_column="0"
                android:layout_row="2"
                android:background="@android:color/transparent"
                android:ems="10"
                android:enabled="false"
                android:inputType=""
                android:textColor="#000"
                android:textSize="24sp" />

            <EditText

                android:id="@+id/mfyerpuan"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_column="0"
                android:layout_row="3"
                android:background="@android:color/transparent"
                android:ems="10"
                android:enabled="false"
                android:inputType=""
                android:textColor="#000"
                android:textSize="24sp" />

            <EditText

                android:id="@+id/tmhampuan"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_column="0"
                android:layout_row="4"
                android:background="@android:color/transparent"
                android:ems="10"
                android:enabled="false"
                android:inputType=""
                android:textColor="#000"
                android:textSize="24sp" />

            <EditText

                android:id="@+id/tmyerpuan"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_column="0"
                android:layout_row="5"
                android:background="@android:color/transparent"
                android:ems="10"
                android:enabled="false"
                android:inputType=""
                android:textColor="#000"
                android:textSize="24sp" />

            <EditText

                android:id="@+id/tshampuan"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_column="0"
                android:layout_row="6"
                android:background="@android:color/transparent"
                android:ems="10"
                android:enabled="false"
                android:inputType=""
                android:textColor="#000"
                android:textSize="24sp" />

            <EditText

                android:id="@+id/tsyerpuan"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_column="0"
                android:layout_row="7"
                android:background="@android:color/transparent"
                android:ems="10"
                android:enabled="false"
                android:inputType=""
                android:textColor="#000"
                android:textSize="24sp" />

            <EditText

                android:id="@+id/dilhampuan"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_column="0"
                android:layout_row="8"
                android:background="@android:color/transparent"
                android:ems="10"
                android:enabled="false"
                android:inputType=""
                android:textColor="#000"
                android:textSize="24sp" />

            <EditText

                android:id="@+id/dilyerpuan"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_column="0"
                android:layout_row="9"
                android:background="@android:color/transparent"
                android:ems="10"
                android:enabled="false"
                android:inputType=""
                android:textColor="#000"
                android:textSize="24sp" />


        </GridLayout>
    </ScrollView>

</android.support.constraint.ConstraintLayout>